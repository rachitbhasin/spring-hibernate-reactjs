{
  "name": "react-redux-starter",
  "version": "1.0.0",
  "description": "React Redux starter project",
  "main": "src/js/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development BABEL_ENV=development webpack --env=development  --progress",
    "prod": "npm run clean && npm run build-prod",
    "build-prod": "cross-env NODE_ENV=production BABEL_ENV=production webpack --env=production -p --progress",
    "clean": "cd ../static && del /S /Q js\\* css\\*",
    "test:clean": "rd /s /q coverage",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "author": "Rachit Bhasin",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.0.4",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "deep-copy": "^1.4.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.2",
    "jest": "^22.0.4",
    "node-sass": "^4.5.3",
    "path": "^0.12.7",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  }
}
